{
  "name": "firestarter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@vimeo/player": "^2.16.2",
    "ajv": "^6.12.6",
    "axios": "^0.18.1",
    "html-react-parser": "^0.4.7",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "react-animations": "^1.0.0",
    "react-dom": "^16.14.0",
    "react-player": "^1.15.3",
    "react-redux": "^5.1.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.1",
    "react-spring": "^4.2.1",
    "react-transition-group": "^2.9.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.4.1",
    "styled-components": "^3.4.10",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/scss -o src/style.css",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o src/style.css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
